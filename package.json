{
  "name": "@justinc/remark-yaml-meta",
  "version": "1.0.0",
  "description": "Extract YAML front-matter from markdown.",
  "license": "MIT",
  "keywords": [
    "yaml",
    "options",
    "meta",
    "settings",
    "remark"
  ],
  "dependencies": {
    "js-yaml": "^3.5.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/akagomez/remark-yaml-meta.git"
  },
  "bugs": "https://github.com/akagomez/remark-yaml-meta/issues",
  "author": "Chris Gomez <tituswormer@gmail.com> (http://akagomez.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "engines": {
    "node": ">=0.10.0"
  },
  "files": [
    "index.js"
  ],
  "devDependencies": {
    "browserify": "^13.0.0",
    "eslint": "^2.0.0",
    "esmangle": "^1.0.0",
    "istanbul": "^0.4.0",
    "jscs": "^3.0.0",
    "jscs-jsdoc": "^2.0.0",
    "remark": "^7.0.1",
    "remark-cli": "^3.0.1",
    "remark-comment-config": "^5.0.0",
    "remark-github": "^7.0.0",
    "remark-html": "^6.0.0",
    "remark-lint": "^6.0.0",
    "remark-slug": "^4.0.0",
    "remark-usage": "^5.0.0",
    "remark-validate-links": "^6.0.0",
    "tape": "^4.0.0",
    "unified": "^6.1.3"
  },
  "scripts": {
    "build-bundle": "browserify --no-builtins index.js -s remarkYAMLConfig > remark-yaml-meta.js",
    "build-mangle": "esmangle remark-yaml-meta.js > remark-yaml-meta.min.js",
    "build-md": "remark . --quiet --frail",
    "build": "npm run build-md && npm run build-bundle && npm run build-mangle",
    "lint-api": "eslint .",
    "lint-style": "jscs --reporter inline .",
    "lint": "npm run lint-api && npm run lint-style",
    "test-api": "node test.js",
    "test-coverage": "istanbul cover test.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  }
}
